&soc {
	i2c@78b6000 { /* BLSP1 QUP3 */
		aw2013_led@45 {
			aw2013_led,name = "aw2013";
			compatible = "aw2013";
			vdd-supply = <&pm8950_l10>;
			qcom,vdd-io-always-on;
			qcom,vdd-io-lpm-sup;
			qcom,vdd-voltage-level = <2750000 2950000>;
			qcom,vdd-current-level = <200 800000>;
			reg = <0x45>;
			aw2013-irq-gpio = <&msm_gpio 16 0x00>;
		};
	};
};
